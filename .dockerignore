# Root .dockerignore for Songbase

# Data directories (use volumes instead)
.metadata/
.song_cache/
.embeddings/
preprocessed_cache/
songs/

# Virtual environments
.venv/
venv/
lib/
bin/
include/
pyvenv.cfg

# Node modules
frontend/node_modules/
frontend/.next/

# Python cache
__pycache__/
*.py[cod]
*$py.class
*.so

# IDE
.idea/
.vscode/
*.swp
*.swo
*~

# Git
.git/
.gitignore

# Electron (not needed for Docker)
electron/
*.spec

# Development files
.claude/
STATUS/

# OS files
.DS_Store
Thumbs.db

# Environment files (use docker-compose env instead)
.env
.env.local

# Documentation (optional)
docs.md
*.md
!README.md
!CLAUDE.md
